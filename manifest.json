{
  "manifest_version": 3,
  "name": "Lightning Fill",
  "version": "1.0",
  "description": "Autofill form fields with test data",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["data.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
